{
  "name": "completecsm-server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --trace-warnings --exec babel-node ./index.js",
    "ngrok": "node ./dev.js",
    "debug": "cross-env DEBUG=server:* npm start",
    "build": "babel ./index.js ./route.js ./config.js --out-dir dist --source-maps inline && babel source --out-dir dist/source --source-maps inline --copy-files && cp -r public dist/public && cp -r templates dist/templates",
    "serve": "node dist/index.js",
    "migrate": "cd schema && sequelize db:migrate",
    "migrate:undo": "cd schema && sequelize db:migrate:undo:all",
    "seed": "cd schema && sequelize db:seed:all",
    "seed:undo": "cd schema && sequelize db:seed:undo:all",
    "setup": "cd schema && NODE_ENV=test sequelize sequelize db:migrate",
    "pretest": "cd schema && NODE_ENV=test sequelize db:migrate",
    "test": "NODE_ENV=test jest --colors --detectOpenHandles --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "posttest": "cd schema && NODE_ENV=test sequelize db:migrate:undo:all"
  },
